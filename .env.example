# Core API keys
OPENROUTER_API_KEY=
# Backward compatibility for legacy scripts; optional if OPENROUTER_API_KEY is set.
GEMINI_API_KEY=

# Telegram
TELEGRAM_BOT_TOKEN=
TELEGRAM_CHAT_ID=
TELEGRAM_ALLOWED_ACCOUNT=
TELEGRAM_ALLOWED_GROUP=

# Tool gateway
ANTIGRAVITY_URL=http://127.0.0.1:8010
GITHUB_TOKEN=
TOOL_GATEWAY_PORT=8010
GITHUB_DEFAULT_OWNER=huanghuiqiang
GITHUB_DEFAULT_REPO=AntigravityOS
GITHUB_DEFAULT_TIMEZONE=Asia/Shanghai

# Feishu bridge (optional)
FEISHU_APP_ID=
FEISHU_APP_SECRET=
FEISHU_DOC_TOKEN=
FEISHU_BRIDGE_PORT=8001
FEISHU_DAILY_BRIEFING_ENABLED=true
FEISHU_WEEKLY_REPORT_ENABLED=true

# Vault paths
OBSIDIAN_VAULT=/app/data/obsidian_vault
INBOX_FOLDER=00_Inbox

# Runtime thresholds
MIN_SCORE_THRESHOLD=8.0
MODEL_BOUNCER=google/gemini-2.0-flash-001
MODEL_SYNTHESIZER=google/gemini-pro-1.5
BOUNCER_DEDUP_ALERT_THRESHOLD=30.0
BOUNCER_ALERT_SUPPRESS_MINUTES=360
BOUNCER_DEDUP_QUERY_DROP_PREFIXES=utm_
BOUNCER_DEDUP_QUERY_DROP_KEYS=spm,from,igshid,fbclid,gclid,mc_cid,mc_eid,ref

# Daily commit digest to Feishu bot
COMMIT_DIGEST_ENABLED=true
COMMIT_DIGEST_TIMEZONE=Asia/Shanghai
COMMIT_DIGEST_CRON=30 21 * * *
COMMIT_DIGEST_REPOS=huanghuiqiang/AntigravityOS,huanghuiqiang/Cognitive-Bouncer
COMMIT_DIGEST_AUTHORS=
COMMIT_DIGEST_MAX_RETRIES=3
COMMIT_DIGEST_RETRY_BACKOFF_SEC=2
COMMIT_DIGEST_DRY_RUN=false
COMMIT_DIGEST_ALERT_ON_FAILURE=true
COMMIT_DIGEST_INCLUDE_CATEGORIES=true
COMMIT_DIGEST_INCLUDE_RISK=true
COMMIT_DIGEST_RISK_PATHS=scheduler.py,agos/notify.py,docker-compose.yml,apps/tool_gateway/,skills/feishu_bridge/
COMMIT_DIGEST_EXCLUDE_TYPES=
COMMIT_DIGEST_MAX_CLASSIFY_COMMITS=200
COMMIT_DIGEST_MAX_REPORT_COMMITS=200
FEISHU_BOT_WEBHOOK=
FEISHU_BOT_SECRET=
FEISHU_BOT_MSG_TYPE=post

# System alert routing and suppression
NOTIFY_SYSTEM_ALERTS_ENABLED=true
NOTIFY_PROVIDER=feishu
NOTIFY_STARTUP_SILENCE_MINUTES=10
NOTIFY_DEFAULT_COOLDOWN_MINUTES=60
NOTIFY_DEDUP_DB_FILE=/app/data/state/notify.sqlite3

# Optional scheduler/runtime tuning
NLM_TIMEOUT=900
INBOX_ARCHIVE_DONE=true
BACKLOG_THRESHOLD_DAYS=10
